{
  "typescript.showUnused": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascript.jsx",
    "javascriptreact",
    "typescript",
    "typescript.jsx",
    "typescriptreact",
    "css",
    "markdown",
    "json",
    "jsonc"
  ],
  "explorer.icon.enableNerdfont": true,
  "coc.source.word.priority": 1,
  "coc.source.dictionary.priority": 2,
  "markdownlint.onSave": true,
  "yaml.validate": true,
  "snippets.extends": {
    "typescriptreact": ["typescript"]
  },
  "coc.preferences.rootPatterns": [".vim/coc-settings", ".git", "package.json"],
  "languageserver": {
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": ["typescript", "typescriptreact", "graphql"]
    }
  },
  "cSpell.userWords": [
    "Debounced",
    "Inlines",
    "Prefetch",
    "SFDT",
    "Siteimprove",
    "airbyte",
    "clsx",
    "debouncing",
    "fluentsms",
    "jsonl",
    "presave",
    "refetch",
    "unmount",
    "upsert"
  ],
  "snippets.ultisnips.pythonPrompt": false
}
